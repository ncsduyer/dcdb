<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.system.dao.AssetMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.system.model.Asset">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="file_size" property="fileSize" />
        <result column="create_time" property="createTime" />
        <result column="status" property="status" />
        <result column="download_times" property="downloadTimes" />
        <result column="file_key" property="fileKey" />
        <result column="filename" property="filename" />
        <result column="file_path" property="filePath" />
        <result column="file_md5" property="fileMd5" />
        <result column="file_sha1" property="fileSha1" />
        <result column="suffix" property="suffix" />
        <result column="more" property="more" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, file_size, create_time, status, download_times, file_key, filename, file_path, file_md5, file_sha1, suffix, more
    </sql>

</mapper>
