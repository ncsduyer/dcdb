<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.system.dao.AppNoticeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.system.model.AppNotice">
        <id column="id" property="id"/>
        <result column="title" property="title"/>
        <result column="type" property="type"/>
        <result column="content" property="content"/>
        <result column="step" property="step"/>
        <result column="createtime" property="createtime"/>
        <result column="tel" property="tel"/>
        <result column="sendee" property="sendee"/>
        <result column="sender_id" property="sender_id"/>
        <result column="now_status" property="now_status"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, type, content,step, createtime, tel, sendee,sender_id,now_status
    </sql>

</mapper>
