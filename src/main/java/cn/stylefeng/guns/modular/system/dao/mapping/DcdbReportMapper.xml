<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.system.dao.DcdbReportMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.system.model.DcdbReport">
        <id column="id" property="id"/>
        <result column="date_group" property="dateGroup"/>
        <result column="title" property="title"/>
        <result column="company" property="company"/>
        <result column="created_time" property="createdTime"/>
        <result column="end_time" property="endTime"/>
        <result column="agent" property="agent"/>
        <result column="requirement" property="requirement"/>
        <result column="remarks" property="remarks"/>
        <result column="work_type" property="workType"/>
        <result column="delay_status" property="delayStatus"/>
        <result column="status" property="status"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, date_group, title, company, created_time, end_time, agent, requirement, remarks, work_type, delay_status, status
    </sql>

</mapper>
